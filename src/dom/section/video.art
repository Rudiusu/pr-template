<div class="list-wrap">
  {{each value}}
  <div class="video-item list-edit">
    {{include "../list-editor.art"}}
    <input class="emily-tpl-input text-value" value="{{ $value }}" placeholder="请输入视频url" />
    <video class="emily-tpl-video" src="{{ $value }}" controls></video>
  </div>
  {{/each}}
</div>
